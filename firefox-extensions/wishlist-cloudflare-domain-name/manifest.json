{
  "manifest_version": 2,
  "name": "Wishlist-CFD",
  "version": "1.0",

  "description": "Put all of your wanted domain names from cloudflare.com in one place",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://dash.cloudflare.com/*/domains/register"],
      "js": ["wishlist.js"]
    }
  ]
}
